{
  "manifest_version": 2,
  "name": "sofy",
  "description": "Stackoverflow.com extension - possible answerers will be added to questions.",
  "version": "1.2",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://www.google-analytics.com/",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://stackoverflow.com/unanswered", "http://stackoverflow.com/questions/*/*", "https://stackoverflow.com/unanswered", "https://stackoverflow.com/questions/*/*"],
      "js": ["jquery.js", "google-analytics-bundle.js", "sofy.js"]
    }
  ]
}